<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="KsiazkaXAMARIN.NewDBPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Nazwa:" Margin="15, 15, 15, 5"/>
            <Entry x:Name="eName" Keyboard="Text" TextChanged="eName_TextChanged" Margin="15, 15, 15, 5"/>
            <Button Text="OK" x:Name="AddButton" Clicked="AddButton_Clicked" WidthRequest="60" HeightRequest="60" Margin="15, 80, 15, 5"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>